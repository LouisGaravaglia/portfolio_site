ed(true);\n\n    if (previewURL !== 'Not a Track') {\n      audio.current.play();\n      audio.current.volume = audioVolume;\n    };\n  };\n\n  //PAUSE THE AUDIO ON MOUSE-LEAVE\n  const stopMusic = () => {\n    \n    if (previewURL !== 'Not a Track') {\n      audio.current.pause();\n    };\n  };\n\n  return (\n    <animated.span className=\"Hover-Box\" onMouseEnter={trigger} onMouseLeave={stopMusic} style={style}>\n      {children}\n    </animated.span>\n  );\n};\n\nexport default Hover;","/Users/Louieg3/Documents/WEB DEVELOPMENT/my_portfolio_site/portfolio/src/Navbar.js",["42","43"],"import React, {useEffect, useState} from \"react\";\n\n\nconst NavBar = () => {\n\n\n\n\n  return (\n    <>\n      <div className=\"Navbar-Container-Left\">\n        <div className=\"Navbar-Search-Box\">\n       \n        </div>\n      </div>\n      <div className=\"Navbar-Container-Right\">\n        <div className=\"Navbar-Browse-Box\">\n      \n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default NavBar;",["44","45"],"/Users/Louieg3/Documents/WEB DEVELOPMENT/my_portfolio_site/portfolio/src/App.js",[],["46","47"],"/Users/Louieg3/Documents/WEB DEVELOPMENT/my_portfolio_site/portfolio/src/hooks/useViewport.js",[],"/Users/Louieg3/Documents/WEB DEVELOPMENT/my_portfolio_site/portfolio/src/Albums.js",["48","49","50"],"/Users/Louieg3/Documents/WEB DEVELOPMENT/my_portfolio_site/portfolio/src/SearchResult.js",[],"/Users/Louieg3/Documents/WEB DEVELOPMENT/my_portfolio_site/portfolio/src/SearchResultList.js",["51","52"],{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","replacedBy":"56"},{"ruleId":"57","severity":1,"message":"58","line":1,"column":45,"nodeType":"59","messageId":"60","endLine":1,"endColumn":56},{"ruleId":"57","severity":1,"message":"61","line":1,"column":16,"nodeType":"59","messageId":"60","endLine":1,"endColumn":25},{"ruleId":"57","severity":1,"message":"62","line":1,"column":27,"nodeType":"59","messageId":"60","endLine":1,"endColumn":35},{"ruleId":"53","replacedBy":"6